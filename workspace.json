{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "equi",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "strict": true,
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint",
        "strict": true
      }
    }
  },
  "projects": {
    "equi": {
      "root": "apps/equi",
      "sourceRoot": "apps/equi/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": ["{options.outputPath}"],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired build",
            "cwd": "apps/equi",
            "outputPath": "dist/apps/equi"
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired start",
            "cwd": "apps/equi"
          }
        },
        "lint": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/eslint src/**/*.tsx src/**/*.ts",
            "cwd": "apps/equi"
          }
        },
        "test": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired test --watchAll=false",
            "cwd": "apps/equi"
          }
        }
      }
    },
    "equi-e2e": {
      "root": "apps/equi-e2e",
      "sourceRoot": "apps/equi-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx e2e-serve equi-react-e2e",
              "nx e2e-run equi-react-e2e"
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/equi-e2e/**/*.{js,ts}"]
          }
        },
        "e2e-run": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/equi-e2e/cypress.json",
            "tsConfig": "apps/equi-e2e/tsconfig.e2e.json",
            "baseUrl": "http://localhost:3000"
          }
        },
        "e2e-serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["nx serve equi-react"],
            "readyWhen": "can now view"
          }
        }
      }
    }
  }
}
